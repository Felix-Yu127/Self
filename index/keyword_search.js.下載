function rUrl(){var o=$.trim($("#keyword_search").val()),e=$("#keyword_type").val(),n=window.location.hostname.split(".").slice(-3).join(".");"請輸入關鍵字搜尋"==o&&(o=""),""!=(n="findComp"==e?"https://www.".concat(n,"/cust/list/index/?jobsource=my104&keyword=").concat(encodeURIComponent(o)):"https://www.".concat(n,"/jobs/search/?jobsource=my104&order=1&keyword=").concat(encodeURIComponent(o)))&&window.open(n)}function cb(){sb=!1,setTimeout(function(){sb=!0},800)}$(function(){$("#keyword_search").keyup(function(o){13==o.keyCode&&rUrl()}),$("#keyword_btn").click(function(){rUrl()})});var as_json1=new bsn.AutoSuggest("keyword_search",{script:"https://".concat(window.location.hostname,"/my104/nccTool/suggestKeyWord?json=true&jobsource=my104&p=10&"),varname:"kw",json:!0,shownoresults:!1,minchars:1,timeout:1e4,callback:cb}),as_div_keep=!1;dv=$("div.autosuggest"),dv&&(dv.mouseover(function(){as_div_keep=!0}),dv.mouseout(function(){as_div_keep=!1})),$(this).keydown(function(o){9==o.keyCode&&$("div.autosuggest").hide()}),$(document).click(function(){$("div.autosuggest")&&!as_div_keep&&$("div.autosuggest").hide()}),$(window).resize(function(){dv=$("div.autosuggest"),dv&&dv.hide()});